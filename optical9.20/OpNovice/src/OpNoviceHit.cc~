#include "OpNoviceHit.hh"
#include "G4UnitsTable.hh"
#include "G4VVisManager.hh"

G4ThreadLocal G4Allocator<OpNoviceHit>*OpNoviceHitAllocator;

OpNoviceHit::OpNoviceHit(){}
OpNoviceHit::~OpNoviceHit(){}
OpNoviceHit::OpNoviceHit(const OpNoviceHit& right)
  :G4VHit()
{
  edep = right.edep;
}
const OpNoviceHit& OpNoviceHit::operator=(const OpNoviceHit& right)
{
  edep = right.edep;
  return this;
}

G4int OpNoviceHit::operator==(const OpNoviceHit& right)const
{
  return(this == &right)?1:0;
}
